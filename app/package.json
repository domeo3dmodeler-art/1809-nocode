{
  "name": "domeo",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "dev": "next dev -p 3000",
    "dev:replit": "next dev -H 0.0.0.0 -p 3000",
    "build": "next build",
    "start": "next start -p 3000",
    "postinstall": "prisma generate || true",
    "seed:sql": "psql \"$DATABASE_URL\" -v ON_ERROR_STOP=1 -f sql/dev_bootstrap_doors.sql",
    "lint": "next lint",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "prisma:seed": "tsx prisma/seed.ts",
    "db:init": "npm run prisma:generate && npm run prisma:migrate:deploy && npm run prisma:seed",
    "db:reset": "prisma migrate reset --force",
    "deploy:yandex": "../scripts/deploy-yandex.sh",
    "dev:quick": "npm run db:init && npm run dev",
    "guard:openapi": "python3 scripts/spec_guard.py --spec openapi.yaml",
    "smoke": "bash scripts/smoke.sh"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.490.0",
    "@aws-sdk/s3-request-presigner": "^3.896.0",
    "@prisma/client": "^5.22.0",
    "@sparticuz/chromium": "^138.0.2",
    "bcryptjs": "^2.4.3",
    "csv-parse": "^6.1.0",
    "dotenv": "^17.2.3",
    "exceljs": "^4.4.0",
    "handlebars": "^4.7.8",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.452.0",
    "next": "14.2.5",
    "next-themes": "^0.2.1",
    "puppeteer-core": "^24.22.0",
    "react": "^18.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "sharp": "^0.33.2",
    "winston": "^3.11.0",
    "xlsx": "^0.18.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/handlebars": "^4.0.40",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "20.14.10",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@types/winston": "^2.4.4",
    "autoprefixer": "10.4.19",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.5",
    "postcss": "8.4.39",
    "prisma": "^5.22.0",
    "tailwindcss": "3.4.10",
    "tsx": "^4.7.0",
    "typescript": "5.5.4"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
